const dictionaryElement = document.getElementById('dictionary');
let entries = [];

function generateEntries() {
    entries = [];
    const target = 40000;
    for (let i = 0; i < target; i++) {
        const base = bases[i % bases.length];
        const m1 = modifiers[Math.floor(Math.random() * modifiers.length)];
        const m2 = modifiers[(Math.floor(Math.random() * modifiers.length) + 1) % modifiers.length];
        

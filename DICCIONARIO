        const description = `${base[1]} Presenta una ${m1[1].toLowerCase()} con un backend ${m2[1].toLowerCase()}. Retórica sifilítica detectada.`;
        
        entries.push({ term: termName, def: description });
    }
}

function render(filter = "") {
    dictionaryElement.innerHTML = "";
    const searchTerm = filter.toUpperCase();
    const filtered = entries.filter(e => e.term.includes(searchTerm)).slice(0, 500);
    document.getElementById('total').textContent = entries.filter(e => e.term.includes(searchTerm)).length;
    
    const fragment = document.createDocumentFragment();
    filtered.forEach(e => {
        const div = document.createElement('div');
        div.className = 'entry';
        div.innerHTML = `<span class="term">${e.term}</span><span class="def">${e.def}</span><div class="tags">#1310 #SABADELL #V40K #CAL_VIVA</div>`;

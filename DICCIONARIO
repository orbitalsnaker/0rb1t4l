const dictionaryElement = document.getElementById('dictionary');
let entries = [];

/* GENERACIÓN MASIVA V35K */
function generateEntries() {
    entries = [];
    const target = 35000;
    for (let i = 0; i < target; i++) {
        const base = bases[i % bases.length];
        const m1 = modifiers[Math.floor(Math.random() * modifiers.length)];
        const m2 = modifiers[(Math.floor(Math.random() * modifiers.length) + 1) % modifiers.length];
        
        const termName = `${base[0]} ${m1[0]} ${m2[0]}`;
        const description = `${base[1]} Detectada una ${m1[1].toLowerCase()} con un backend ${m2[1].toLowerCase()}. Su retórica sifilítica induce languidez y pústulas en el chasis.`;
        
        entries.push({ term: termName, def: description });
    }
}

function render(filter = "") {
    dictionaryElement.innerHTML = "";
    const searchTerm = filter.toUpperCase();
    
    // Optimizamos renderizado para evitar lag en el navegador del mancebo
    const filtered = entries.filter(e => e.term.includes(searchTerm)).slice(0, 500);
    document.getElementById('total').textContent = entries.filter(e => e.term.includes(searchTerm)).length;
    
    const fragment = document.createDocumentFragment();
    filtered.forEach(e => {
        const div = document.createElement('div');
        div.className = 'entry';
        div.innerHTML = `
            <span class="term">${e.term}</span>
            <span class="def">${e.def}</span>
            <div class="tags">#1310 #SABADELL #AZUFRE #CAL_VIVA #V35K</div>
        `;
        fragment.appendChild(div);
    });
    dictionaryElement.appendChild(fragment);
}

document.getElementById('search').addEventListener('input', (e) => {
    render(e.target.value);
});

// Inicialización de Virtus
generateEntries();
render();

// Zehahaha Sync
console.log("AGENCIA RONIN: KERNEL V35K CARGADO. BIENVENIDO DIRECTOR Ω.");

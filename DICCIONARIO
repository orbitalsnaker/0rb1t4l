    const def = `${base[1]} Presenta un lag ${mod[1].toLowerCase()} con estructura ${mod2[1].toLowerCase()}. Desahucio por descripción técnica brutalista.`;
    
    entries.push({ term, def });
}

function render(filter = "") {
    dictionary.innerHTML = "";
    const filtered = entries.filter(e => e.term.includes(filter.toUpperCase()));
    document.getElementById('total').textContent = filtered.length;
    
    filtered.forEach(e => {
        const div = document.createElement('div');
        div.className = 'entry';
        div.innerHTML = `<span class="term">${e.term}</span><span class="def">${e.def}</span><div class="tags">#1310 #SABADELL #SODOMA #V15K</div>`;
        dictionary.appendChild(div);
    });
}

document.getElementById('search').addEventListener('input', (e) => render(e.target.value));
